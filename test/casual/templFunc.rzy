

function narrow{low,high:Integer}( x: R{-1000,10000} ):R{low,high}
  if (x >= low) and (x <= high) then
    return R{low,high}( x );
  else
    return low;
  end
end


component
  TemplFunc
    response in( x: R{0,20} ):R{0,10};
    
    implementation elementary
    
    response in( x: R{0,20} ):R{0,10}
      return narrow{0,10}( x );
    end

  end

