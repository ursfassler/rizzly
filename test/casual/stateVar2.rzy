component
  StateVar2
    input
      in( x: R{0,10} );
    
    output
      out( x: R{0,10} );
    
  implementation elementary
    
    var
      a : R{0,10};
      
    entry
      a := 0;
    end
    
    function addret( x: R{0,10} ):R{0,10}
      a := (a + 1) mod 11;
      return x;
    end
    
    function in( x: R{0,10} )
      a := addret( x );
      out( a );
    end
    
  end

