StateVar2 = Component
  out : signal( x: R{0,10} );
  ind : slot( x: R{0,10} );

elementary
  a : R{0,10} = 0;
      
  add = procedure( x: R{0,10} )
    a := (a + 1) mod 11;
  end
  
  ret = function( x: R{0,10} ):R{0,10}
    return x;
  end
  
  ind : slot( x: R{0,10} )
    add( x );
    a := ret( x );
    out( a );
  end
  
end

