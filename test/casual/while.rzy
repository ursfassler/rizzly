Point = Record
  x : R{0,255};
  y : R{0,255};
end

While = Component
  out : query( x: R{0,255} ):Point;
  ind : response( x: R{0,255} ):Point;

elementary
  ind : response( x: R{0,255} ):Point
    ret : Point;
    ret.x := 0;
    ret.y := 0;
    i : R{0,256} = 0;
    while i < x do
      ret := out( x );
      i := R{1,256}( i+1 );
    end
    return ret;
  end
end

