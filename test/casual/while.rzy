type
  Point = Record
    x : R{0,255};
    y : R{0,255};
  end

component
  While

    input
      in( x: R{0,255} ):Point;

    output
      out( x: R{0,255} ):Point;

    implementation elementary

    function in( x: R{0,255} ):Point
      ret : Point;
      ret.x := 0;
      ret.y := 0;
      while x > 0 do
        ret := out( x );
        x := x-1;
      end
      return ret;
    end
  end

