type
  Point = Record
    x : R{0,255};
    y : R{0,255};
  end

interface
  Iface0
    function foo( x: R{0,255} ):Point;
  end

component
  While

    input
      in    : Iface0;

    output
      out   : Iface0;

    implementation elementary

    function in.foo( x: R{0,255} ):Point
      ret : Point;
      ret.x := 0;
      ret.y := 0;
      while x > 0 do
        ret := out.foo( x );
        x := x-1;
      end
      return ret;
    end
  end

