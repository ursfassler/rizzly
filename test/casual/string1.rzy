component
  String1
    input
      tick();
    
    output
      text( x: String );
    
  implementation elementary
    
    var
      k : R{0,2};
      
    entry
      k := 0;
    end
    
    function tick()
      case k of
        0: text( 'Hello' ); end
        1: text( ' ' ); end
        2: text( 'World' ); end
      end
      k := (k + 1) mod 3;
    end
    
  end

