component
  Case

    input
      ind( data: R{0,100} );

    output
      out( data: R{0,100} );

    implementation elementary

    function ind( x: R{0,100} )
      case x of
        0:
          out( 2 );
        end
        1:
          out( 0 );
        end
        3..10,12:
          out( 1 );
        end
        2,11,20:
          out( 10 );
        end
        15,30..40:
          out( 12 );
        end
        else
          out( 8 );
        end
      end
    end

  end

