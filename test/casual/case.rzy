Case = Elementary
  out : signal( data: R{0,100} );

  ind : slot( x: R{0,100} )
    case x of
      0:
        out( 2 );
      end
      1:
        out( 0 );
      end
      3..10,12:
        out( 1 );
      end
      2,11,20:
        out( 10 );
      end
      15,30..40:
        out( 12 );
      end
      else
        out( 8 );
      end
    end
  end

end

