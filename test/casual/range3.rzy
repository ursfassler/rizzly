component
  Range3
    input
      in( x: R{0,10} ):R{0,3};
    
    implementation elementary
    
    function in( x: R{0,10} ):R{0,3}
      if (x > 3) and (x <= 7) then
        return R{0,3}( x - 4 );
      ef x > 9 then
        return R{0,3}( x - 9 );
      ef x > 5 then
        return R{0,3}( x - 6 );
      else
        return R{0,3}( x );
      end
    end
    
  end

