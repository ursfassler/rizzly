calcTable = function(n: R{0,100}):Array{10,R{0,100}}
  res : Array{10,R{0,100}};
  i   : R{0,11} = 0;
  while i < 11 do
    idx : R{0,10} = R{0,10}(i);
    res[idx] := idx * n / 10;
    i := idx + 1;
  end
  return res;
end

ConstInit6 = Elementary
  lookuptable : const = calcTable(57);
    
  get : response( idx: R{0,10} ):R{0,100}
    return lookuptable[idx];
  end
end      

