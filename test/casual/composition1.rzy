DummyComp = Component
  out : signal();
  ind : slot();

elementary
  ind : slot()
    out();
  end

end

Composition1 = Component
  ind : slot();
  out : signal();

composition
  a : DummyComp;
  b : DummyComp;
  c : DummyComp;
  
  ind -> a.ind;
  ind -> b.ind;
  a.out -> b.ind;
  a.out -> c.ind;
  c.out -> out;
  b.out -> out;
end

