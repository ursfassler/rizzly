interface
  Iface
    function get():R{0,50};
  end

component
  ElemInit 
    input
      in  : Iface;

  implementation elementary
    
    var
      x : R{0,50};
      
    entry
      x := 42;
    end
    
    exit
      x := 23;
    end
    
    function in.get():R{0,50}
      return x;
    end

  end

