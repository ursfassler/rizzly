type
  Point = Record
    x : R{0,100};
    y : R{0,100};
  end

component
  Rec1
    input
      ind( x: R{0,100} );
    
    output
      out( x: R{0,100} );
    
  implementation elementary

    function ind( x: R{0,100} )
      p : Point;
      p.x := x;
      p.y := 100 - p.x;
      out( p.y );
    end

  end

