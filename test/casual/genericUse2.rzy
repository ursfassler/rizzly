component
  GenericDef{T:Type{Integer}}
    input
      set( item: T );
      get():T;

    implementation elementary

    var
      data: T;

    function set( item: T )
      data := item;
    end

    function get():T
      return data;
    end
  end
  
  GenericUse2
    implementation elementary
    component
      gen : GenericDef{R{0,255}};

    function foo()
      gen.set( 42 );
      a : R{0,255} = gen.get();
    end
  end

