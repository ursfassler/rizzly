GenericDef{T:Type{Integer}} = Component
  set : slot( item: T );
  get : response():T;

elementary
  data: T = 0;

  set : slot( item: T )
    data := item;
  end

  get : response():T
    return data;
  end
end

GenericUse2 = Component
  set : slot( x: R{0,255} );
  get : response():R{0,255};
  
composition
  gen : GenericDef{R{0,255}};
    
  set -> gen.set;
  get -> gen.get;
end

