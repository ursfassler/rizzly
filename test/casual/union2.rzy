Uni = Union
  pos  : R{0,10};
  neg  : R{-10,10};
end

Union2 = Elementary
  data : Uni = [pos := 1];

  abs = function( x: R{-10,10} ):R{0,10}
    if x < 0 then
      return R{0,10}( -x );
    else
      return R{0,10}( x );
    end
  end

  ind : slot()
    x : R{0,10};
    case data of
      Uni.pos:
        x := data.pos;
      end
      Uni.neg:
        x := abs( data.neg );
      end
    end
  end
  

end


