
getBits = function( nr: Natural ):Natural
  bits : Natural = 0;
  while nr <> 0 do
    nr   := nr shr 1;
    bits := bits + 1;
  end
  return bits;
end

Rec{T:Type{Any}} = Record
  a : T;
  b : T;
end

Typefunc = Elementary
  a : R{0,getBits( 42 )} = 0;
  b : R{0,255} = 0;
  c : Rec{R{0,255}} = [a := 0, b := 0];
end

