DummyComp = Component
  out : query():R{0,100};
  ind : response():R{0,100};

elementary
  ind : response():R{0,100}
    return out();
  end
end

CompositionQuery = Component
  ind : response():R{0,100};
  out : query():R{0,100};

composition
  a : DummyComp;
  b : DummyComp;
    
  ind -> a.ind;
  a.out -> b.ind;
  b.out -> out;
end

