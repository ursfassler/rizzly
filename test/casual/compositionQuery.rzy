interface
  Iface
    function foo( x: R{0,255}; y: R{0,8191} );
    function bar( x: R{0,65535} ):R{0,32767};
  end

component
  DummyComp
    input
      in  : Iface;
    
    output
      out : Iface;

    implementation elementary

    function in.foo( x: R{0,255}; y: R{0,8191} )
      out.foo( x, y );
    end

    function in.bar( x: R{0,65535} ):R{0,32767}
      return out.bar( x );
    end
    
  end

  CompositionQuery
    input
      in  : Iface;
    
    output
      out : Iface;

    implementation composition
    
    component
      a : DummyComp;
      b : DummyComp;
    
    connection
      in -> a.in;
      a.out -> b.in;
      b.out -> out;
  end

