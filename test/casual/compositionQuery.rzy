interface
  Iface
    function bar():R{0,100};
  end

component
  DummyComp
    input
      in  : Iface;
    
    output
      out : Iface;

    implementation elementary

    function in.bar():R{0,100}
      return out.bar();
    end
    
  end

  CompositionQuery
    input
      in  : Iface;
    
    output
      out : Iface;

    implementation composition
    
    component
      a : DummyComp;
      b : DummyComp;
    
    connection
      in -> a.in;
      a.out -> b.in;
      b.out -> out;
  end

