

something = function( x: R{0,255} ):R{0,255}
  return (x + 2) mod 256;
end

Answer : const R{0,255} = something( 42 );

ConstInit = Component
composition
  comp1 : Dummy{1};
  comp2 : Dummy{Answer/2};
  test  : Test;
end
  
Test = Component
elementary
  BlaBla : const R{0,255} = something( 23 );
  BluBlu : const R{0,255} = Answer;
  BleBle : const R{0,Answer} = 1;
  BloBlo : const R{0,255} = (Answer + 10) / 3;

  a : R{0,Answer}   = 0;
  b : R{0,Answer+2} = 0;
end      

Dummy{S:R{0,255}} = Component
elementary
  Dodo : const R{0,S} = 1;
  Dudu : const R{0,255} = S;
  Dada : const R{-100,100} = something(S) / 2 - 100;
    
  a	: R{0,S}        = 0;
  b : R{0,(S+10)/2} = 0;
    
  foo = procedure()
    bar : R{0,255} = Answer;
    bar := something( bar );
  end

end


