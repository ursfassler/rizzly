
interface
  Tick
    function tack( x: R{0,255} ):R{0,255};
  end
  
component
  Range2
    input
      in  : Tick;
    
    implementation elementary
    
    function in.tack( x: R{0,255} ):R{0,255}
      t : R{0,510} = x * 2;
      if t > 255 then
        t := 255;
      end
      return R{0,255}( t );
    end

  end

