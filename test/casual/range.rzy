
interface
  Tick
    function tick( x: R{0,10} );
  end
  
component
  Range
    input
      in  : Tick;
    
    output
      out : Tick;
      
    implementation elementary
    
    var
      a : R{0,10};
    
    function in.tick( x: R{0,10} )
      if a < 10 then
        a := a + 1;
        out.tick( a );
      else
        out.tick( x );
      end
    end
    
  end

