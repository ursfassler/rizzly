
component
  Range
    input
      in( x: R{0,10} );
    
    output
      out( x: R{0,10} );
      
  implementation elementary
    
    var
      a : R{0,10};
    
    function in( x: R{0,10} )
      if a < 10 then
        a := R{0,10}( a + 1 );
        out( a );
      else
        out( x );
      end
    end
    
  end

