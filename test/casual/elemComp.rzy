
component
  Sub{Init,Deinit:R{0,100}}
    input
      get():R{0,100};
      
  implementation elementary
    var
      value : R{0,100};
    
    entry
      value := Init;
    end
    
    exit
      value := Deinit;
    end
    
    function get():R{0,100}
      return value;
    end
  end

  ElemComp
    input
      val1():R{0,100};
      val2():R{0,100};
      val3():R{0,100};
      val4():R{0,100};
      val5():R{0,100};
      
  implementation elementary
    component
      sub1 : Sub{10,20};
      sub2 : Sub{20,30};
      sub3 : Sub{30,40};
      sub4 : Sub{40,50};
      sub5 : Sub{50,60};
    
    function val1():R{0,100}
      return sub1.get();
    end
    
    function val2():R{0,100}
      return sub2.get();
    end
    
    function val3():R{0,100}
      return sub3.get();
    end
    
    function val4():R{0,100}
      return sub4.get();
    end
    
    function val5():R{0,100}
      return sub5.get();
    end
  end

