
Sub{Init,Deinit:R{0,100}} = Component
  get : response():R{0,100};

elementary
  value : R{0,100} = 0;
  
  entry
    value := Init;
  end
  
  exit
    value := Deinit;
  end
  
  get : response():R{0,100}
    return value;
  end
end

ElemComp = Component
  val1 : response():R{0,100};
  val2 : response():R{0,100};
  val3 : response():R{0,100};
  val4 : response():R{0,100};
  val5 : response():R{0,100};
    
composition
  sub1 : Sub{10,20};
  sub2 : Sub{20,30};
  sub3 : Sub{30,40};
  sub4 : Sub{40,50};
  sub5 : Sub{50,60};
  
  val1 -> sub1.get;
  val2 -> sub2.get;
  val3 -> sub3.get;
  val4 -> sub4.get;
  val5 -> sub5.get;

end

