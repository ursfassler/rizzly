
component
  Sub{Init,Deinit:R{0,100}}
    response get():R{0,100};
      
  implementation elementary
    var
      value : R{0,100} = 0;
    
    entry
      value := Init;
    end
    
    exit
      value := Deinit;
    end
    
    response get():R{0,100}
      return value;
    end
  end

  ElemComp
    response val1():R{0,100};
    response val2():R{0,100};
    response val3():R{0,100};
    response val4():R{0,100};
    response val5():R{0,100};
      
  implementation composition
    component
      sub1 : Sub{10,20};
      sub2 : Sub{20,30};
      sub3 : Sub{30,40};
      sub4 : Sub{40,50};
      sub5 : Sub{50,60};
    
    connection
      val1 -> sub1.get;
      val2 -> sub2.get;
      val3 -> sub3.get;
      val4 -> sub4.get;
      val5 -> sub5.get;

  end

