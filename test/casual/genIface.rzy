GenIface = Elementary
  out1in  : signal( x: R{0,10} );
  out1out : query():R{0,10};
  out2in  : signal( x: R{0,2000} );
  out2out : query():R{0,2000};

  in1in : slot( x: R{0,10} )
    out1in( x );
  end

  in1out : response():R{0,10}
    return out1out();
  end

  in2in : slot( x: R{0,2000} )
    out2in( x );
  end

  in2out: response():R{0,2000}
    return out2out();
  end

end

