component
  GenIface
    slot     in1in( x: R{0,10} );
    response in1out():R{0,10};
    slot     in2in( x: R{0,2000} );
    response in2out():R{0,2000};
    signal   out1in( x: R{0,10} );
    query    out1out():R{0,10};
    signal   out2in( x: R{0,2000} );
    query    out2out():R{0,2000};

  implementation elementary

    slot in1in( x: R{0,10} )
      out1in( x );
    end

    response in1out():R{0,10}
