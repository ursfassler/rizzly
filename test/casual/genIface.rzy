import
  iface.iface2;

component
  GenIface

    input
      in1    : Iface2{U{8}};
      in2    : Iface2{U{16}};

    output
      out1   : Iface2{U{8}};
      out2   : Iface2{U{16}};

    implementation elementary

    function in1.in( x: U{8} )
      out1.in( x );
    end

    function in1.out():U{8}
      return out1.out();
    end

    function in2.in( x: U{16} )
      out2.in( x );
    end

    function in2.out():U{16}
      return out2.out();
    end

  end

