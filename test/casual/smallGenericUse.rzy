type
  TheRecord{T:Type{Natural}} = Record
    a : T;
    b : R{0,255};
  end

  The2Record{T:Type{Natural};R:Type{Any}} = Record
    a : T;
    b : R;
  end

  The3Record{R:Type{Any}} = Record
    a : R{0,255};
    b : R;
  end

component
  SmallGeneric{T:Type{Natural}}
    implementation elementary

    var
      dat1: TheRecord{T};
      dat2: The2Record{T,Boolean};
      dat3: The2Record{T,T};
      dat4: The3Record{Boolean};
      dat5: The3Record{TheRecord{T}};
      dat6: The3Record{The2Record{T,Boolean}};
      dat7: The3Record{The2Record{T,TheRecord{T}}};
      a: T;
  end

  SmallGenericUse
    implementation composition

    component
      gen1 : SmallGeneric{R{0,255}};
      gen2 : SmallGeneric{R{0,65535}};
    
  end

