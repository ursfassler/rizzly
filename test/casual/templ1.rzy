
Templ1 = Component
composition
  sub1 : SubTempl1{Answer};
  sub2 : SubTempl2{R{0,func1{func2{Answer+1}()}(4)+Answer}, func1{Answer}(SomeNumber)*2};
end

SubTempl1{N:Natural} = Component
composition
  sub3 : SubTempl2{R{0,2*N}, N};
end

SubTempl2{Q:Type{Integer}; M:Integer} = Component
composition
end

Answer : const R{0,func1{SomeNumber}(3)} = 5;
SomeNumber : const = func2{11}();

func1{V:Natural} = function(n: Integer):R{0,V}
  return n+3;
end

func2{S:R{0,func1{10}(11)}} = function():R{0,2*S}
  return S + 1;
end

