Schmitt{Lower,Upper: R{0,1000}} = Hfsm
  ind : slot( value: R{0,1000} );
      
  state( Low )
    Low  : state;
    High : state;
    
    Low to High by ind( value: R{0,1000} ) do
      a : R{0,1000} = Upper;
    end
    High to Low by ind( value: R{0,1000} ) if value <= Lower;
  end
end
  
Param = Composition
  ind : slot( value: R{0,1000} );
  
  schmitt : Schmitt{90,110};
      
  ind -> schmitt.ind;
end
    
