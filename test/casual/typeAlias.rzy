
type
  Byte = R{0,255};
  Word = R{0,65535};
  Unsigned{T:Type{Natural}} = T;
  
interface
  Iface
    function foo();
  end
  
component
  TypeAlias
    input
      in  : Iface;
  
    implementation elementary

    var
      a : Byte;
      b : Word;
      c : Unsigned{R{0,4294967295}};
      
    function in.foo()
      a := 0;
      b := 0;
      c := 0;
    end
    
  end

