
type
  Byte = R{0,255};
  Word = R{0,65535};
  Unsigned{T:Type{Natural}} = T;
  Byte2 = Byte;
  
interface
  Iface
    function foo();
  end
  
component
  TypeAlias
    input
      in  : Iface;
  
    implementation elementary

    var
      a : Byte;
      b : Word;
      c : Unsigned{R{0,4294967295}};
      d : Unsigned{R{0,255}};
      e : Byte2;
      
    function in.foo()
      a := 0;
      b := 0;
      c := 0;
      d := a;
      a := d;
    end
    
  end

