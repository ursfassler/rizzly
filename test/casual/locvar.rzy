
interface
  Tick
    function tack( x: R{0,255} ):R{0,255};
  end
  
component
  Locvar
    input
      in  : Tick;
    
    implementation elementary
    
    function in.tack( x: R{0,255} ):R{0,255}
      if x = 0 then
        return 0;
      else
        t : R{0,510} = x * 2;
        if t > 255 then
          t := 255;
        end
        return t;
      end
      return 0;
    end

  end

