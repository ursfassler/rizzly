type
  Point = Record
    x : R{0,100};
    y : R{0,100};
  end

component
  Rec2
    input
      getX( p: Point ):R{0,100};
      getY( p: Point ):R{0,100};
    
  implementation elementary

    function getX( p: Point ):R{0,100}
      return p.x;
    end

    function getY( p: Point ):R{0,100}
      return p.y;
    end

  end

