type
  Point = Record
    x : R{0,100};
    y : R{0,100};
  end

component
  Rec2
    response getX( p: Point ):R{0,100};
    response getY( p: Point ):R{0,100};
    
  implementation elementary

    response getX( p: Point ):R{0,100}
      return p.x;
    end

    response getY( p: Point ):R{0,100}
      return p.y;
    end

  end

