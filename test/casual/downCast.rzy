
component
  DownCast
    input
      a( x: R{0,50000} ):R{0,200};
      b( x: R{0,50000} ):R{0,200};
      c( x: R{0,50000} ):R{0,200};
      d( x: R{0,50000} ):R{0,200};
    
    implementation elementary
    
    function a( x: R{0,50000} ):R{0,200}
      return x and 127;
    end
    
    function b( x: R{0,50000} ):R{0,200}
      return x mod 201;
    end

    function c( x: R{0,50000} ):R{0,200}
      return x and 31;
    end
    
    function d( x: R{0,50000} ):R{0,200}
      return x mod 42;
    end

  end

