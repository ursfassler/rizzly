
interface
  Tick
    function tick( x: R{0,50000} ):R{0,200};
  end
  
component
  DownCast
    input
      a   : Tick;
      b   : Tick;
      c   : Tick;
      d   : Tick;
    
    implementation elementary
    
    function a.tick( x: R{0,50000} ):R{0,200}
      return x and 127;
    end
    
    function b.tick( x: R{0,50000} ):R{0,200}
      return x mod 201;
    end

    function c.tick( x: R{0,50000} ):R{0,200}
      return x and 31;
    end
    
    function d.tick( x: R{0,50000} ):R{0,200}
      return x mod 42;
    end

  end

