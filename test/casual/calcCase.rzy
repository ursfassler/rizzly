component
  CalcCase

    input
      in( data: R{0,20} );

    output
      out( data: R{0,20} );

    implementation elementary

    function in( x: R{0,20} )
      case x+2 of
        2..10:
          out( 2 );
        end
        11:
          out( 3 );
        end
        12..22:
          out( 4 );
        end
      end
    end

  end

