interface
  Tick
    function tick( data: R{0,20} );
  end

component
  CalcCase

    input
      in    : Tick;

    output
      out   : Tick;

    implementation elementary

    function in.tick( x: R{0,20} )
      case x+2 of
        2..10:
          out.tick( 2 );
        end
        11:
          out.tick( 3 );
        end
        12..22:
          out.tick( 4 );
        end
      end
    end

  end

