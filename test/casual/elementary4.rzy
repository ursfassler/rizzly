//test ok

component
  Elementary4

  input
    evtIn( value: R{0,255} );
    
  output
    evtOut( value: R{0,255} );

  implementation elementary

    function sqr( v: R{0,200} ):R{0,200}
      return v*v mod 201;
    end

    function evtIn( value: R{0,255} )
      evtOut( sqr( value mod 201 ) );
    end
  end
  
