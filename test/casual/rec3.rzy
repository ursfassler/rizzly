Point = Record
  x : R{0,100};
  y : R{0,100};
end

Rec3 = Elementary
  getElem : response( p: Point; elem: R{0,1} ):R{0,100}
    case elem of
      0: return p.x; end
      1: return p.y; end
      else return 0; end  //TODO remove this when Rizzly is capable of testing case options
    end
  end

end

