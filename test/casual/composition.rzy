component
  DummyComp
    slot   ind();
    signal out();

  implementation elementary

    slot ind()
      out();
    end

  end

  Composition
    slot   ind();
    signal out();

  implementation composition
    
    component
      a : DummyComp;
      b : DummyComp;
      c : DummyComp;
    
    connection
      ind -> a.ind;
      ind -> b.ind;
      a.out -> b.ind;
      a.out -> c.ind;
      c.out -> out;
      b.out -> out;
  end

