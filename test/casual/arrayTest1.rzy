interface
  Tick
    function tick( x: R{0,200} );
  end

component
  ArrayTest1
    input
      in  : Tick;
      
    output
      out : Tick;

  implementation elementary

    var
      a : Array{20,R{0,200}};
    
    entry
      i : R{0,20} = 0;
      while i < 20 do
        a[i] := i * 10;
        i    := R{0,20}( i + 1 );
      end
    end

    function in.tick( x: R{0,200} )
      out.tick( a[x mod 20] );
    end

  end

