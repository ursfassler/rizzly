component
  StateVariable2

  input
    tick();

  implementation hfsm(A)

    state A
      var
        a : R{0,255};
        
      entry
        a := 0;
      end
      
      function tuwas()
        a := (a + 2) and 255;
      end
      
      transition 
        A to A by tick() do
          a := (a + 1) mod 256;
        end
    end
  end

