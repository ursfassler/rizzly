import
  valueEvent;

component
  Elementary4

  input
    evtIn   : ValueEvent;
    
  output
    evtOut  : ValueEvent;

  implementation elementary

    function sqr( v: R{0,200} ):R{0,200}
      return v*v mod 201;
    end

    function evtIn.evt( value: R{0,255} )
      evtOut.evt( sqr( value mod 201 ) );
    end
  end
  
