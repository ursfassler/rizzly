component
  Query4
    input
      tick();
      read( x: R{0,10} ):R{0,100};
    
  implementation hfsm( A )
  
    transition
      A to B by tick();
      B to C by tick();
      C to A by tick();
  
    state A
      function read( x: R{0,10} ):R{0,100}
        return x * 1;
      end
    end
      
    state B
      function read( x: R{0,10} ):R{0,100}
        return x * 2;
      end
    end

    state C
      function read( x: R{0,10} ):R{0,100}
        return x * 3;
      end
    end
    
  end

