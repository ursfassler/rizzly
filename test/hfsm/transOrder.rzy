TransOrder = Component
  inp : slot( val: R{0,63} );
  out : signal( val: R{0,63} );
    
hfsm(A)
  testBit = function( value: R{0,63}; bit: R{0,5} ):Boolean
    return (value and (1 shl bit)) <> 0;
  end

  A to A by inp( val: R{0,63} ) if testBit( val, 0 ) do
    out( 0 );
  end
  
  A to A by inp( val: R{0,63} ) if testBit( val, 1 ) do
    out( 1 );
  end
  
  A : state
    A to A by inp( val: R{0,63} ) if testBit( val, 2 ) do
      out( 2 );
    end
    
    A to A by inp( val: R{0,63} ) if testBit( val, 3 ) do
      out( 3 );
    end
  end

  A to A by inp( val: R{0,63} ) if testBit( val, 4 ) do
    out( 4 );
  end
  
  A to A by inp( val: R{0,63} ) if testBit( val, 5 ) do
    out( 5 );
  end
    
end

