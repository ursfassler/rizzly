//test ok

interface
  ValueEvent
    function tick( val: R{0,5} );
  end

component
  TransOrder

  input
    in      : ValueEvent;
  
  output
    out     : ValueEvent;
    
  implementation hfsm(A)
    function testBit( value: R{0,63}; bit: R{0,5} ):Boolean
      return (value and (1 shl bit)) <> 0;
    end
  
    transition 
      A to A by in.tick( val: R{0,5} ) if testBit( val, 0 ) do
        out.tick( 0 );
      end
      
      A to A by in.tick( val: R{0,5} ) if val = 1 do
        out.tick( 1 );
      end
    
    state A
      transition 
        A to A by in.tick( val: R{0,5} ) if val = 2 do
          out.tick( 2 );
        end
        
        A to A by in.tick( val: R{0,5} ) if val = 3 do
          out.tick( 3 );
        end
    end

    transition 
      A to A by in.tick( val: R{0,5} ) if val = 4 do
        out.tick( 4 );
      end
      
      A to A by in.tick( val: R{0,5} ) do
        out.tick( 5 );
      end
      
  end

