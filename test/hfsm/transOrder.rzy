component
  TransOrder

  input
    in( val: R{0,63} );
  
  output
    out( val: R{0,63} );
    
  implementation hfsm(A)
    function testBit( value: R{0,63}; bit: R{0,5} ):Boolean
      return (value and (1 shl bit)) <> 0;
    end
  
    transition 
      A to A by in( val: R{0,63} ) if testBit( val, 0 ) do
        out( 0 );
      end
      
      A to A by in( val: R{0,63} ) if testBit( val, 1 ) do
        out( 1 );
      end
    
    state A
      transition 
        A to A by in( val: R{0,63} ) if testBit( val, 2 ) do
          out( 2 );
        end
        
        A to A by in( val: R{0,63} ) if testBit( val, 3 ) do
          out( 3 );
        end
    end

    transition 
      A to A by in( val: R{0,63} ) if testBit( val, 4 ) do
        out( 4 );
      end
      
      A to A by in( val: R{0,63} ) if testBit( val, 5 ) do
        out( 5 );
      end
      
  end

