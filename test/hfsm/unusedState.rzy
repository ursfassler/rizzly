// State BB, AB, CA, CB, C are never reached

component
  UnusedState

  input
    tick();

  implementation hfsm(A)

    state A(AA)
      state AA
      end

      state AB
      end
    end

    state B(BA)
      state BA
      end

      state BB
      end
    end

    state C(CA)
      state CA
      end

      state CB
      end
    end

    transition 
      A to BA by tick();
      BA to A by tick();
      BB to A by tick();
  end

