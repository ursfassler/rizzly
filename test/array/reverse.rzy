
function reverse{S:Natural;T:Type{Any}}( x: Array{S,T} ): Array{S,T}
  t : T;
  i : R{0,S/2+1} = 0;
  while i < S/2 do
    t         := x[i];
    x[i]      := x[S-1-i];
    x[S-1-i]  := t;
    i         := R{0,S/2+1}( i + 1 );
  end
  return x;
end

component
  Reverse
    response data( x: Array{10,R{0,100}} ): Array{10,R{0,100}};
      
  implementation elementary

    response data( x: Array{10,R{0,100}} ): Array{10,R{0,100}}
      return reverse{10,R{0,100}}( x );
    end

  end

