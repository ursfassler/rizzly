const
  data : Array{10,R{0,100}} = [
    10,
    20,
    1,
    2,
    3,
    4,
    100,
    5,
    12,
    7 
  ];

interface
  Iface
    function foo( x: R{0,100} );
  end

component
  ArrayValue
    input
      in  : Iface;
    
    output
      out : Iface;

    implementation elementary
    
    function in.foo( x: R{0,100} )
      out.foo( data[x] );
    end

  end

