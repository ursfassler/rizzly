type
  Color = Enum
    Red;
    Green;
    Blue;
  end

component
  Enumt
    input
      in( x: R{0,100} );
    
    output
      out( x: Color );

    implementation elementary
    
    function in( x: R{0,100} )
      case x of
        0: out( Color.Red ); end
        1: out( Color.Green ); end
        2: out( Color.Blue ); end
      end
    end
    
  end

