DummyComp = Component
  outfoo : signal();
  outbar : signal();
  outpoh : signal();

  infoo : slot();
  inbar : slot();
  inpoh : slot();

elementary
  infoo : slot()
    outfoo();
    outbar();
  end

  inbar : slot()
    outbar();
    outpoh();
  end

  inpoh : slot()
    outbar();
  end
end

Debug = Component
  infoo : slot();
  inbar : slot();
  inpoh : slot();
  
  outfoo: signal();
  outbar : signal();
  outpoh : signal();

composition
  a : DummyComp;
  b : DummyComp;

  infoo -> a.infoo;
  inbar -> a.inbar;
  inpoh -> a.inpoh;
  infoo -> b.infoo;
  inbar -> b.inbar;
  inpoh -> b.inpoh;

  a.outfoo -> b.infoo;
  a.outbar -> b.inbar;
  a.outpoh -> b.inpoh;
  b.outfoo -> outfoo;
  b.outbar -> outbar;
  b.outpoh -> outpoh;
end

