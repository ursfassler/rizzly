type
  Point = Record
    x : R{0,1000};
    y : R{0,1000};
  end

component
  Structt
    input
      in1( x, y: R{0,1000} );
      in2( x: Point );
    
    output
      out1( x: Point );
      out2( x, y: R{0,1000} );

    implementation elementary
    
    function in1( x, y: R{0,1000} )
      p : Point;
      p.x := x;
      p.y := y;
      out1( p );
    end
    
    function in2( p: Point )
      out2( p.x, p.y );
    end

  end

