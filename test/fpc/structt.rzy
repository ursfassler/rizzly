Point = Record
  x : R{0,1000};
  y : R{0,1000};
end

Structt = Component
  out1 : signal( x: Point );
  out2 : signal( x, y: R{0,1000} );
  in1 : slot( x, y: R{0,1000} );
  in2 : slot( p: Point );

elementary
  in1 : slot( x, y: R{0,1000} )
    p : Point;
    p.x := x;
    p.y := y;
    out1( p );
  end
  
  in2 : slot( p: Point )
    out2( p.x, p.y );
  end
end

